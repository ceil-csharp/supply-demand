<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Sergey Shkatula</Authors>
    <Company>CEIL ceil.dev</Company>
    <PackageId>SupplyDemand</PackageId>
    <Version>0.0.1</Version>
    <Title>SupplyDemand C# Library</Title>
    <Description>C# supply-demand pattern implementation</Description>
    <PackageTags>DependencyInjection;SupplyDemand;Functional</PackageTags>
